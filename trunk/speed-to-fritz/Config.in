### FIRMWARE_CONFIG_ENDE ###
###-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
### ATTENTION! Most variables set up here are in use by various scripts later, so do not change,
### except numbers within variable names corresponding to the above firmware names.
### If a firmware version or subversion changes replace all the partial numbers accuring within this config file.
### Example:  _11779 to _XXXXX.  Or _58 to _XX (but be careful because some of the very short numbers may be used on other ocasins)
choice
	prompt "Hardware type"
		default TYPE_SPEEDPORT_W920V
		help
			Select your box type here.

	config TYPE_SINUS_W500V
		bool "Sinus W500V"
		select TYPE_LANG_DE

	config TYPE_SPEEDPORT_W501V
		bool "Speedport W501V"
		select TYPE_LANG_DE
		select FORCE_TCOM_FON
#		select FORCE_TCOM_DSL
		select FORCE_TCOM_PIGLET
		
	config TYPE_SPEEDPORT_W503V
		bool "Speedport W503V"
		select XCHANGE_KERNEL
	config TYPE_SPEEDPORT_W701V
		bool "Speedport W701V"

	config TYPE_SPEEDPORT_W721V
		bool "Speedport W721V"

	config TYPE_SPEEDPORT_W900V
		bool "Speedport W900V"

	config TYPE_SPEEDPORT_W920V
		bool "Speedport W920V"
#		select SET_FREETZ_REVISION

	config TYPE_7141
		bool "AVM 7141"

	config TYPE_7270v3
		bool "AVM 7270v3"

	config TYPE_7240v2
		bool "AVM 7240"

	config LOCAL_X_MODEL
		bool "Any AVM custom model"

endchoice
if LOCAL_X_MODEL
 config SPMOD
	    string "Any AVM custom model type --->"
	    default "7270"	
	    help 
		    In this case only 1st AVM and 2nd AVM firmware is in use, so insert any AVM model type and 
		    then insert the firmware names for 1st AVM and 2nd AVM firmware.
endif

if  TYPE_7141 || TYPE_7270v3 || TYPE_7240v2 || LOCAL_X_MODEL
    config EXPORT_TYPE_LOCAL_MODEL
	bool
	default y
endif

if ! LOCAL_X_MODEL
    config SPMOD
	string
	default "501"		if TYPE_SPEEDPORT_W501V
	default "503"		if TYPE_SPEEDPORT_W503V
	default "701"		if TYPE_SPEEDPORT_W701V
	default "721"		if TYPE_SPEEDPORT_W721V
	default "900"		if TYPE_SPEEDPORT_W900V
	default "920"		if TYPE_SPEEDPORT_W920V
	default "500"		if TYPE_SINUS_W500V
	default "7270v3"	if TYPE_7270v3
	default "7240v2"	if TYPE_7240v2
	default "7141"		if TYPE_7141
endif

