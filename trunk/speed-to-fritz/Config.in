config EXPORT_FORCE_DSL_MULTI_ANNEX 
	bool "Force Multi annex (optional)"
	depends on ( ! ORI && ! TYPE_SPEEDPORT_W900V && ! TYPE_SPEEDPORT_W701V && ! TYPE_SPEEDPORT_W721V && ! TYPE_SPEEDPORT_W501V && ! TYPE_SINUS_W500V )
	default n if ! TYPE_9207570
	default y if TYPE_9207570
	help
	    To make this work you must have a firmware that supplies the needed feature, firmware with minor version > 70 (7270 like).
	    In case of 7170 Firmwares, it is usual better to stay without this optin because the drivers are optimized for a special annex type.

choice
	depends ! EXPORT_FORCE_DSL_MULTI_ANNEX && ! TYPE_AVM_7570_XX
	bool "Annex"
	depends on ! ORI
	default TYPE_ANNEX_B if ! EXPORT_TYPE_LOCAL_MODEL
	default TYPE_ANNEX_A if EXPORT_TYPE_LOCAL_MODEL
	config TYPE_ANNEX_B
		bool "B"
	help
		Annex parameter, internationally annex A is in use, in Germany this is usually B,
		this is a parameter for DSL and specifies the frequency range of ADSL, POTS lines with 
		ADSL usually are annex A, ISDN lines usually annex B. 
		For Speed-to-Fritz, this parameter will be set correctly only if the automatic FTP push
		option is used in combination or the resulting firmware is transfered via Web interface.
		annex A or B. You would have to enter the annex parameter as FTP command line,
		as 'quote SETENV kernel_args annex=A' but then some telephone settings are not adjusted at the same time.
	config TYPE_ANNEX_A
		bool "A"
	help
		Annex parameter, internationally annex A is in use, in Germany this is usually B,
		this is a parameter for DSL and specifies the frequency range of ADSL, POTS lines with 
		ADSL usually are annex A, ISDN lines usually annex B. 
		For Speed-to-Fritz, this parameter will be set correctly only if the automatic FTP push
		option is used in combination or the resulting firmware is transfered via Web interface.
		Using some other tools to transfer the kernel.image to the box will not work for setting
		annex A or B. You would have to enter the annex parameter as FTP command line,
		as 'quote SETENV kernel_args annex=A' but then some telephone settings are not adjusted at the same time.
endchoice
config ANNEX
	string
	default "A" if TYPE_ANNEX_A 
	default "B" if TYPE_ANNEX_B || EXPORT_FORCE_DSL_MULTI_ANNEX
