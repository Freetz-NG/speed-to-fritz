	depends on TYPE_SINUS_W500V
	default n
	help
		Use FON DRIVER from second source firmware specified in SRC2_IMG.
 config COPY_ADDON_TMP_to_ORI
	bool "Copy addon/tmp with option -u"
	depends on ORI
	default n
	help
		Set this to 'y' if you like to include all files within the directory
		./addon/tmp/squashfs-root in combination with option -u.

 config UPDATE_DEPMOD
	bool "Update module dependency information"
	depends on !ORI
	default n
	help
		Update module dependency information in generated image. Useful if deploy modules
		via addon directories.
 config MOVE_ALL_to_OEM
	bool "MOVE ALL to OEM"
	default y
	depends on ! TYPE_SPEEDPORT_W721V
	help
		Set this to "y" if Freetz 7170 firmwares are used instead of original AVM firmware,
		WWW "all" directory is moved to "$OEM"
		Other OEM WWW directories if present are removed to save space.
		Always turn this on if freetz firmware is used!
		Disable this option if autoconfig is used.
 config MOVE_AVM_to_OEM
	bool "MOVE avm to OEM"
	default n
	depends on TYPE_SPEEDPORT_W920V
	help
		Set this to "y" if normal 7270 firmwares in combination with multilingual firmwares are used, but usual this is not needed.
		WWW "avm" directory is moved to "$OEM"
		
 config EXPORT_CONFIG_TR069
	bool "Enable TR069 disable ATA"
	default n
	help
		Set this to "y" if you want that autoconfiguration turned on,
		this may turn your box back automatically to T-Home firmware 
		if new T-Home firmware is available. If this happens, it is not sure
		that your box will function normal again, so do not use this option 
		expect you be aware of this case.
		You must set OEM to the branding you use.  
		Autoconfiguration is only set on if AVM firmware supports this option!.
		ATA is disabled if tr069 is turned on.
		
 config EXPORT_CONFIG_TR064
	bool "Enable LAN autoconfiguration TR064"
	default n if ! TYPE_SPEEDPORT_W920V
	default y if TYPE_SPEEDPORT_W920V
	help
		Set this to "y" if you want TR64 turned on, in most cases
		your box will function normal again, so do not use this option
		expect you be aware of this case.
		Autoconfiguration from within the internal LAN is only 
		set on if AVM firmware supports this option!.
 config XCHANGE_KERNEL
	bool "Xchange kernel between T-Home and avm source"
	default n if ! TYPE_SPEEDPORT_W721V
	default y if TYPE_SPEEDPORT_W721V
	help
		For normal this is "n", but for testing this may be used, this option is dangerous,
		but it can be used in combination with the option restore original firmware as well.
		It is mainly added for testing W920, W920 original firmware does not log to the console
		but if the kernel is replaced with AVM kernel the log is present.
endif
 config SRC2_KERNEL
	bool "Take kernel from 2nd AVM source"
	depends on SOURCE2 && ! XCHANGE_KERNEL
	default n
	help
		For normal this is "n", but for testing this may be used, this option is dangerous,
		but it can be used in combination with the option restore original firmware as well.
		It is mainly added for testing W920, W920 original firmware does not log to the console
		but if the kernel is replaced with AVM kernel the log is present.
 config COPY_ADDON_TMP
	bool "Copy addon/tmp"
	depends on !ORI
	default n
	help
		Set this to 'y' if you like to include all files within the directory ./addon/tmp/squashfs-root.
config EXPORT_FORCE_CLEAR_FLASH
	bool "Clear mtd3 and mtd4"
	default y
	help
		Set this to "y" if firmwares are used witch are older or much newer than the one installed.
		Usually this is also made automatically based on version numbers but in some cases
		versions will not indicate the changes made by the script. This will prevent a reboot loop,
		caused by settings incompatibility. 

comment "TEST Options -------------------------------------------------------"

config VERBOSEETY
	bool "Verbose display for debugging"
	default y
	help
		Verbose display for debugging, script may be interrupted if this is set!

config SET_UP
	bool "Just do a setup run"
	default n
	help
		No output file will be generated! Process stops before packing firmware!.

config EXPORT_REUSECONF
	bool "Keep configuration"
	default n
	help
		Firmware.conf is reused if menu is restarted.

config DO_NOT_STOP_ON_ERROR
	bool "Do not STOP on ERROR" 
