#	depends on TYPE_LABOR
#	string "If you change between W920V and back read this help!"
#	default "ATTENTION!"
#	help
#		-------------------------------------------------------------------------
#		ATTENTION! AVM uses the same Filenames for 7170 and 7270 Labor-firmwares!
#		So if you change your settings from W920V (in combination with a
#		Labor firmware 11500) back to any other or the other way round, then
#		the previously loaded Labor firmware located in /Firmware.org is removed
#		-------------------------------------------------------------------------
#!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
#### If types are added here the script file /subscripts/settings has to be edited as well to represent the new Labor versions too. 
config TYPE_LABOR
	bool "Select a special AVM firmware (optional) --->"
	depends on ! ORI || XCHANGE_KERNEL
	depends on ( TYPE_SINUS_W500V || TYPE_OR_503_701_721_722_900 || TYPE_OR_7570_920_LOCALTCOM_LOCALAVM )
	default y if EXPORT_TYPE_LOCAL_MODEL || ( TYPE_SPEEDPORT_W920V && TYPE_LANG_DE ) || TYPE_OR_7390_722 || TYPE_SPEEDPORT_W503V 
	default n if ! EXPORT_TYPE_LOCAL_MODEL && ! ( TYPE_SPEEDPORT_W920V && TYPE_LANG_DE ) && ! TYPE_7390 && ! TYPE_SPEEDPORT_W722V && ! TYPE_SPEEDPORT_W503V
	help
		Enable this to compile the mod based on an special AVM firmware.
		The actual final release version should not be selected. It is preselected anyway! 
		If you select it here your freetz config is not correct! 
choice
	prompt "Select Labor or any other non default firmware."
		depends on TYPE_LABOR
		default TYPE_7272_54_04_80 if EXPORT_TYPE_LOCAL_MODEL && ! TYPE_7141 && ! TYPE_7240
		default TYPE_7170_29_04_80 if ( EXPORT_TYPE_LOCAL_MODEL && TYPE_7141 ) || ( ! TYPE_SPECIALW721_TCOM && TYPE_SPEEDPORT_W721V )
		default TYPE_7170_29_04_80 if TYPE_7140
		default TYPE_7272_54_04_59 if TYPE_SPEEDPORT_W503V
		default TYPE_7170_58_04_80 if ( ! TYPE_SPECIALW721_TCOM && TYPE_SPEEDPORT_W721V )
		default TYPE_AIO_7272_14543 if ! EXPORT_TYPE_LOCAL_MODEL
#		default TYPE_PRE_7272_18158 if TYPE_OR_7570_920 && ! FORCE_VDSL
#		default TYPE_PRE_7272_18158 if TYPE_OR_7570_920 && FORCE_VDSL
		default TYPE_PRE_7272_17828 if TYPE_OR_7570_920
		default TYPE_PRE_7273_18158 if TYPE_7240
		#->(only for testing)
		default TYPE_7390_PRE_18101 if TYPE_7390
#		default TYPE_7390_84_04_84 if TYPE_7390
		default TYPE_W722_80_04_76 if TYPE_SPEEDPORT_W722V
		#<--

		
