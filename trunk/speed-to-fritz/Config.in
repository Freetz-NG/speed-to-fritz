### ATTENTION! Most variables set up here are in use by various skripts later, so don't change, 
### except numbers within variable names corresponding to the above firmware names.
### If a firmware version or subversion changes replace all the partial numbers accuring within this configfile.
### Example:  _11779 to _XXXXX.  Or _58 to _XX 
###------------------------------------------------------------------------------------------------------------

choice
	prompt "Hardware type"
		default TYPE_SPEEDPORT_W701V
		help
			Select your box type here.

	config TYPE_SINUS_W500V
		bool "Sinus W500V"
		select TYPE_LANG_DE

	config TYPE_SPEEDPORT_W501V
		bool "Speedport W501V"
		select TYPE_LANG_DE
		select FORCE_TCOM_FON
		select FORCE_TCOM_DSL
		select FORCE_TCOM_PIGLET
		
	config TYPE_SPEEDPORT_W503V
		bool "Speedport W503V"
		select XCHANGE_KERNEL
	config TYPE_SPEEDPORT_W701V
		bool "Speedport W701V"

	config TYPE_SPEEDPORT_W721V
		bool "Speedport W721V"

	config TYPE_SPEEDPORT_W900V
		bool "Speedport W900V"

	config TYPE_SPEEDPORT_W920V
		bool "Speedport W920V"
#		select SET_FREETZ_REVISION

	config TYPE_TCOM_LOCAL
		bool "Custom model"
endchoice
if TYPE_TCOM_LOCAL
    config SPMOD
        string "Custom TCOM Type"
	default "701"	
	help 
	    Enter your model type 500, 501, 503, 701, 721, 900 or 920 only for this models patches are installed, 
	    if you use this patches on other future models it is not sure what the results will be like.
endif
if ! TYPE_TCOM_LOCAL
config SPMOD
	string
	default "501"	if TYPE_SPEEDPORT_W501V
	default "503"	if TYPE_SPEEDPORT_W503V
	default "701"	if TYPE_SPEEDPORT_W701V
	default "721"	if TYPE_SPEEDPORT_W721V
	default "900"	if TYPE_SPEEDPORT_W900V
	default "920"	if TYPE_SPEEDPORT_W920V
	default "500"	if TYPE_SINUS_W500V
endif
config TCOM_LOCAL
    depends on TYPE_TCOM_LOCAL
    string "Enter your custom type TCOM like Firmware"
    default "./Firmware.orig/..."
    help 
	Enter your custom type TCOM like firmware, in the same manner with path/filename as usually.

