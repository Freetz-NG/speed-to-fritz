config EXPORT_CONFIG_DSL_MULTI_ANNEX
	bool "Force Multiannex"
	depends on ( TYPE_SRC3_AVM_7570_70 || TYPE_TCOM_7570_70 || TYPE_LOCAL_TCOM ) && ! TYPE_AVM_7570_70
	default n
